<?xml version="1.0"?>
<extension key="alternativenamesearch" type="module">
  <file>alternativenamesearch</file>
  <name>Alternative Name Search</name>
  <description>This extension allows searching for names in any order. It also handles searching for duplicate names (where someone has been given the same name twice).</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Rich Lott</author>
    <email>forums@artfulrobot.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/artfulrobot/alternativenamesearch</url>
    <url desc="Documentation">https://github.com/artfulrobot/alternativenamesearch</url>
    <url desc="Support">https://github.com/artfulrobot/alternativenamesearch<Paste></url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2019-02-14</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>This requires the patchwork extension</comments>
  <requires>
    <ext>patchwork</ext>
  </requires>
  <civix>
    <namespace>CRM/Alternativenamesearch</namespace>
  </civix>
</extension>
